package com.tnsif.day8.polymorphism;

public class overloading {
	int num1;
	int num2;
	int result;
	
	void sum(int a,int b) 
	{
		num1 =a;
		num2=b;
		result=num1+num2;
		
		System.out.println("The results are:"+result);
	}
	
	void sum(int a,double b) 
	{
		num1 =a;
		num2=(int)b;
		result=num1+num2;
		
		System.out.println("The results are:"+result);
	}
	public static void main(String[] args) {
		
	}

}
